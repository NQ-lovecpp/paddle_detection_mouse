{
  "compilerOptions": {
    "target": "es2019",
    "module": "esnext",
    "jsx": "react-native",
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": ["react", "react-native"]
  },
  "exclude": ["node_modules"]
}
